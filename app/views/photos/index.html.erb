<h1>Photo Album</h1>

<div class="album">
  <% @places.each do |place| %>
    <div class="photo">
      <%= link_to maps_index_path(place) do %>
        <%= attachment_image_tag place, :image, :fill, 100, 100, format: 'jpeg' %>
      <% end %>
      <div>
        <%= link_to "削除", photo_path(place), method: :delete, data: { confirm: '本当に削除しますか？' } %>
      </div>
    </div>
  <% end %>
</div>
